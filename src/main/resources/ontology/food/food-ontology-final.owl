    </owl:AnnotationProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/stefan-kehayov/food-ontology#hasBase -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/stefan-kehayov/food-ontology#hasBase">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/stefan-kehayov/food-ontology#hasIngredient"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/stefan-kehayov/food-ontology#isBaseOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/stefan-kehayov/food-ontology#Pizza"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/stefan-kehayov/food-ontology#PizzaBase"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/stefan-kehayov/food-ontology#hasCountryOfOrigin -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/stefan-kehayov/food-ontology#hasCountryOfOrigin">
