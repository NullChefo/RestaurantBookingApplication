    <owl:Class rdf:about="http://www.semanticweb.org/stefan-kehayov/food-ontology#CheesePizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/stefan-kehayov/food-ontology#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/stefan-kehayov/food-ontology#hasTopping"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/stefan-kehayov/food-ontology#CheeseTopping"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/stefan-kehayov/food-ontology#VeganPizza"/>
    </owl:Class>
    


