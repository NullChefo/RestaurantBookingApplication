        <rdfs:domain rdf:resource="http://www.semanticweb.org/stefan-kehayov/food-ontology#Pizza"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/stefan-kehayov/food-ontology#Country"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/stefan-kehayov/food-ontology#hasIngredient -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/stefan-kehayov/food-ontology#hasIngredient">
