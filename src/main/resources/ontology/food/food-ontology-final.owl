    <!-- http://www.semanticweb.org/stefan-kehayov/food-ontology#AnchovyPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/stefan-kehayov/food-ontology#AnchovyPizza">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/stefan-kehayov/food-ontology#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/stefan-kehayov/food-ontology#hasSpice"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/stefan-kehayov/food-ontology#OreganoSpice"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/stefan-kehayov/food-ontology#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/stefan-kehayov/food-ontology#AnchovyTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/stefan-kehayov/food-ontology#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/stefan-kehayov/food-ontology#MozzarelaTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/stefan-kehayov/food-ontology#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/stefan-kehayov/food-ontology#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/stefan-kehayov/food-ontology#hasCalorieValue"/>
                <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</owl:hasValue>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">anchovy pizza</rdfs:label>
        <rdfs:label xml:lang="bg">пица аншоа</rdfs:label>
    </owl:Class>
    


