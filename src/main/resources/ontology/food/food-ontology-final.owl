        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/stefan-kehayov/food-ontology#Food"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/stefan-kehayov/food-ontology#Food"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/stefan-kehayov/food-ontology#hasSpice -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/stefan-kehayov/food-ontology#hasSpice">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/stefan-kehayov/food-ontology#hasIngredient"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/stefan-kehayov/food-ontology#isSpiceOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/stefan-kehayov/food-ontology#Pizza"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/stefan-kehayov/food-ontology#Spice"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/stefan-kehayov/food-ontology#hasTopping -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/stefan-kehayov/food-ontology#hasTopping">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/stefan-kehayov/food-ontology#hasIngredient"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/stefan-kehayov/food-ontology#isToppingOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/stefan-kehayov/food-ontology#Pizza"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/stefan-kehayov/food-ontology#PizzaTopping"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/stefan-kehayov/food-ontology#isBaseOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/stefan-kehayov/food-ontology#isBaseOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/stefan-kehayov/food-ontology#isIngredientOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/stefan-kehayov/food-ontology#PizzaBase"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/stefan-kehayov/food-ontology#Pizza"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/stefan-kehayov/food-ontology#isIngredientOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/stefan-kehayov/food-ontology#isIngredientOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/stefan-kehayov/food-ontology#Food"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/stefan-kehayov/food-ontology#Food"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/stefan-kehayov/food-ontology#isSpiceOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/stefan-kehayov/food-ontology#isSpiceOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/stefan-kehayov/food-ontology#isIngredientOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/stefan-kehayov/food-ontology#isToppingOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/stefan-kehayov/food-ontology#isToppingOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/stefan-kehayov/food-ontology#isIngredientOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:ObjectProperty>
    


